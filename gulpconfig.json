{
  "browserSyncOptions" : {
    "proxy": "http://localhost/8888/",
    "notify": true
  },
  "browserSyncWatchFiles" : [
    "./css/*.min.css",
    "./js/*.min.js",
    "./**/*.php"
  ],
  "paths" : {
    "js": "./js",
    "css": "./css",
    "img": "./img",
    "imgsrc": "./src/img",
    "sass": "./sass",
    "node": "./node_modules/",
    "bower": "./bower_components/",
    "dev": "./src",
    "php": "./**/*.php",
    "dist": "./dist",
    "distprod": "./dist-product",
    "vendor": ""
  },
  "project": {
    "name": "wowgulpwpstarter",
    "languagefolder": "./languages"
  }
}
